            <script language="JavaScript">
                $(function () {
                    var cale = new Calendar("idCalendar", {
                        onSelectDay: function (o) { o.className = "onSelect"; },
                        onToday: function (o) { o.className = "onToday"; },
                        onFinish: function () {
                            _cld("idCalendarYear").innerHTML = this.Year; _cld("idCalendarMonth").innerHTML = this.Month;
                            var y = this.Year; var m = String(this.Month).padLeft(2, '0'); var flag; var cal = this;
                            $.ajax({
                                async: false, url: "/dabian/dbht", data: { year: y, month: m }, type: "post",
                                success: function (text) {
                                    var flag = text; for (var i = 0, len = flag.length; i < len; i++) {
                                        cal.Days[flag[i]].innerHTML = "<a href='dbh?rq=" + "{0}-{1}-{2}".format(y, m, String(flag[i]).padLeft(2, '0')) + "'>" + flag[i] + "</a>";
                                    }
                                },
                                error: function (jqXHR, textStatus, errorThrown) { alert(jqXHR.responseText); }
                            });
                        }
                    });
                    _cld("idCalendarPre").onclick = function () { cale.PreMonth(); }
                    _cld("idCalendarNext").onclick = function () { cale.NextMonth(); }
                });
            </script>

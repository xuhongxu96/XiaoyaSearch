        <script>
            var url = $.url().attr('path'); var param = $.url().param(); param.model = 1; param._model= Math.random() ; $.ajaxSettings.async = false;
            $.getJSON(url,param, function(msg) {
                if (msg.message) alert(msg.message); if (msg.script) eval(msg.script); if (msg.href) window.open(msg.href, msg.target);
                if (msg.success) {
                    viewModel = window.ViewModel ? window.ViewModel(msg.data) : msg.data;
                    try { ko.applyBindings(viewModel); } catch (e) { alert(e.message); }
                }
            });
            $.ajaxSettings.async = true;
        </script>

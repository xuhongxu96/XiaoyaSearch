<script>
	$(window).scroll(function(){
        var top =$(window).scrollTop();
        $(".header").addClass("fix-on header-collapsed");
        if(top <= 0){
            $(".header").removeClass("fix-on header-collapsed");
        }
    });
	

    function checkfrmm(){
        var search1 = document.getElementById("search_input1").value;
        if (search1 == '' ) {
            $("#frmm").find('input').attr("placeholder","请输入文本");
            return false;
        }
    }

    $('.ui.dropdown.lang').dropdown({action: 'combo'});
</script>

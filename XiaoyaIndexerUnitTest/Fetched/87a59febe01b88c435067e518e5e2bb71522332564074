		$(document).ready( function() {
			
			  var swiper = new Swiper('.swiper-container',{
			    pagination: '.pagination',
			    loop:true,
			    grabCursor: true,
			    paginationClickable: true,
			//    autoplay : 5000,
			//    autoplayDisableOnInteraction : false,
			    
			  })
				$('.swiper-button-prev').on('click', function(e){
				  e.preventDefault()
				  swiper.swipePrev()
				})
				$('.swiper-button-next').on('click', function(e){
				  e.preventDefault()
				  swiper.swipeNext()
				})
				$('.swiper-slide').mouseover(function(){
					swiper.stopAutoplay();
				})
				$('.swiper-slide').mouseout(function(){
					swiper.startAutoplay();
				})
			
		});
		$(document).ready( function() {
		
		
			  // moment.locale('ru');
			  var myclndr;
			  // here's some magic to make sure the dates are happening this month.
			  var thisMonth = moment().format('YYYY-MM');
		
			  
			  function showPop(month){
				  
					//this.setEvents(eventArray);
					    $.getJSON('http://career.bnu.edu.cn/front/career/meetDate/getDate.jspa', {yearMonth:month}, function(data) {
				     // alert(data);
				      
				      var objArr = [];
				      for(var i = 0; i < data.length; i++ ){
				      	objArr[i] = { date: data[i]};
				      }
				      myclndr.setEvents(objArr);
				      
				      $('.event').webuiPopover({
				          type:'async',
				          url:'http://career.bnu.edu.cn/front/career/meetDate/getListMeet.jspa',
				         reqData:function(ele){
				 			   
				 			   var str =ele.attr('class');
				 			   //alert(str);
				 			   var beginOf=str.indexOf("calendar-day-")+13;
				 			   var endOf=beginOf+10;
				 		       var result=str.substring(beginOf,endOf);
				             var reqData = {yearMonthDay:result};
				             return reqData;
				          },
				          content:function(result){
		
				          	var dataJson = $.parseJSON(result);
								var dataset =dataJson.data;
								//alert(dataset);
				              var html = '<ul>';
				              
				             for(var i = 0; i < dataset.length; i++){
				                  html+='<li><a  href="/front/zph.jspa?tid='+dataset[i].tid+'">'+dataset[i].name+'</a></li>';
				              }
				              html+='</ul>';
				              return html;
				             
				          }   
				      });
				  });  
			  }
		
				    
			  myclndr = $('.cal1').clndr({
			    template: $('#template-calendar').html(),
			    
			    clickEvents: {
			      
			      onMonthChange: function(month) {
			    	  var mon = month.format('YYYY-MM');
			    	  showPop(mon);
		
			      },
			      nextYear: function() {

			      },
			      previousYear: function() {

			      },
			      onYearChange: function() {

			      }
			    },
			    multiDayEvents: {
			      startDate: 'startDate',
			      endDate: 'endDate',
			      singleDay: 'date'
			     },
			    showAdjacentMonths:true,
			    ready: function() { 
			    	var month = moment().format('YYYY-MM');
			    	//alert(mon);
			    	showPop(month);  
				},
			    adjacentDaysChangeMonth: false,
		
			  });
		
		});
		
		var session_memberTypeId = '';
		
		$(function(){
			//验证是否已经登录，未登录弹出登录窗口
			$('.v_login').click(function() {

				var returnUrl = $(this).attr('returnUrl');
				var memberTypeId = $(this).attr('memberTypeId');
				
				if(session_memberTypeId == ''){
					
					if(typeof(returnUrl) != 'undefined'){
						returnUrl = returnUrl.replace("http://career.bnu.edu.cn" , "");
						var target = "/membermanage/toPopLogin.jspa?memberTypeId="+memberTypeId+"&returnUrl="+encodeURIComponent(returnUrl);
					}else{
						var target = "/membermanage/toPopLogin.jspa?callback=changeButton";
					}
			 	    
			        art.dialog.open(target, {
			            lock:true,
			            fixed:true,
			            id: 'import',
			            opacity: 0.5,
			            width: 500,
			            height: 360,
			            title: '账号登录'
			        });	
					return false;
				}else{
					if(typeof(memberTypeId) != 'undefined' && memberTypeId != session_memberTypeId){
						if(memberTypeId == '86'){
							alert('此功能需要学生账号登录');
						}
						if(memberTypeId == '87'){
							alert('此功能需要用人单位账号登录');
						}
						if(memberTypeId == '88'){
							alert('此功能需要院系账号登录');
						}
						return false;
					}else{
						//alert('您已登录');
					}
					
					return true;
				}
			});		
		});
		
		
		function changeButton(){
			
			var list = art.dialog.list; for (var i in list) { list[i].close(); };
			$('#login').hide();
			$('#logout').show();
			//alert(111)
		}
		
		//检测登录
		$.getJSON('http://career.bnu.edu.cn/membermanage/checkSession.jspa', {}, function(data) {
			if(data.msg == 'Y'){
				var vo = jQuery.parseJSON(data.vo);
				//alert(vo.memberTypeId);
				if(vo.memberTypeId == 86){

				}
/* 				if(vo.memberTypeId == 87){
					//alert(vo.name);
					$(".name").text(vo.name);
					
					$('#empForm').hide().siblings().show();
				}
				if(vo.memberTypeId == 88){
					//alert(vo.name);
					$(".name").text(vo.vo);
					
					$('#collegeForm').hide().siblings().show();
				} */
				$('#logout').show();
				$('#login').hide();
			}
		});
		
	</script>

        function XiaoUrl() {
            var friend = "";
            $.ajax({
                async: false,
                type: "post",
                url: "../Handler/Ajax.ashx",
                data: {
                    methodname: "GetFriendUrl",
                    Types: "1",
                },
                success: function (msg) {
                    $.each($.parseJSON(msg), function (i, item) {
                        friend += '<option value="' + item.Address + '">' + item.Conntent + '</option>'
                    })
                    $("#xiaoUrl").append(friend);
                }
            })
        }
        $("#xiaoUrl").change(function () {
            var url = this.value;
            window.location.href = url;
        })

        function FenUrl() {
            var friend = "";
            $.ajax({
                async: false,
                type: "post",
                url: "../Handler/Ajax.ashx",
                data: {
                    methodname: "GetFriendUrl",
                    Types: "2",
                },
                success: function (msg) {
                    $.each($.parseJSON(msg), function (i, item) {
                        friend += '<option value="' + item.Address + '">' + item.Conntent + '</option>'
                    })
                    $("#fenUrl").append(friend);
                }
            })
        }
        $("#fenUrl").change(function () {
            var url = this.value;
            window.location.href = url;
        });

        $(".hp_inlink_option option").mouseover(function () {
            $(this).css("background", "#559567");
        });
    </script>


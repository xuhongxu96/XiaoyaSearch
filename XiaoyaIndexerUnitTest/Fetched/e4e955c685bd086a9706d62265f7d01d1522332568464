    <script type="text/javascript">
        function Close() {
            $(".mengban").hide();
            $(".mengban2").hide();
        }
        function tuichu() {
            debugger;
            $.ajax({
                async: false,
                type: "post",
                url: "/PersonalCenter/Login.html",
                data: {
                    actionName: "Out"
                },
                success: function (msg) {
                    debugger;
                    if (msg) {
                        if (msg == "1") {
                            history.go(0);
                        }
                    }
                }
            });
        }

        function ClickCount(id) {
            $.ajax({
                async: false,
                type: "post",
                url: "/Handler/Ajax.ashx",
                data: {
                    methodname: "ClickCount",
                    id: id
                },
                success: function (msg) {
                }
            });
        }

        $(function () {
            //判断浏览器是否支持placeholder属性
            supportPlaceholder = 'placeholder' in document.createElement('input'),
                placeholder = function (input) {
                    var text = input.attr('placeholder'),
                        defaultValue = input.defaultValue;
                    if (!defaultValue) {
                        input.val(text).addClass("phcolor");
                    }
                    input.focus(function () {
                        if (input.val() == text) {
                            $(this).val("");
                        }
                    });
                    input.blur(function () {
                        if (input.val() == "") {
                            $(this).val(text).addClass("phcolor");
                        }
                    });
                    //输入的字符不为灰色
                    //input.keydown(function () {
                    //    $(this).removeClass("phcolor");
                    //});
                };
            //当浏览器不支持placeholder属性时，调用placeholder函数
            if (!supportPlaceholder) {
                $('input').each(function () {
                    text = $(this).attr("placeholder");
                    if ($(this).attr("type") == "text") {
                        placeholder($(this));
                    }
                    if ($(this).attr("type") == "password") {
                        placeholder($(this));
                    }
                });
            }
        });


        if (navigator.appName == "Microsoft Internet Explorer" && navigator.appVersion.match(/8./i) == "8.") {
            $(".header-input").val("请输入搜索内容");
            $(".header-input").css("background", "#ccc");
        }
        else if (navigator.appName == "Microsoft Internet Explorer" && navigator.appVersion.match(/9./i) == "9.") {
            $(".header-input").val("请输入搜索内容");
            $(".header-input").css("background", "#ccc");
        }
        $(".nav_bar>li").hover(function () {
            $(this).find(".nav-ul2").show().siblings().children(".nav-ul2").hide();
            $(this).find(".nav-horn").show().parent().siblings().children(".nav-horn").hide();
        }, function () {
            $(this).find(".nav-ul2").hide();
            $(".nav-horn").hide();
            $(".nav-horns").hide();
        });
        $(".nav_bar>li").eq(0).hover(function () {
            $(".nav-horn").hide();
        });
        $(".nav_bar>li>.nav-ul2>li").hover(function () {
            $(this).addClass("nav-ul2-active").siblings().removeClass("nav-ul2-active");
            $(this).parent().parent().find(".nav-horns").eq(0).show().parent().siblings().children(".nav-horns").hide();
        }, function () {
            $(this).removeClass("nav-ul2-active");
        });
        $(".nav_bar li:eq(0)").hover(function () {
            $(this).find(".nav-ul2").hide();
        });
        function EditImg() {
            var imglist = $("#fellowImg").find("img");
            $(imglist).each(function () {
                $(this).click(function () {
                    $(".mask").css("display", "block")
                    var maskImg = $(this).attr("src");//图片路径
                    $(".mask #maskImgShow").attr("src", maskImg);//原图显示
                    var maskImgWidthyu = $(".mask #maskImgShow").width();
                    var maskImgHeightyu = $(".mask #maskImgShow").height();//原图的宽高
                    var ClienWidthOff = document.body.offsetWidth;
                    var ClienHeightOff = document.body.offsetHeight;//浏览器宽高

                    if (maskImgWidthyu > ClienWidthOff) {
                        var ClienWidth = (parseInt(ClienWidthOff) - 30) + "px";
                        $(".mask #maskImgShow").attr("width", ClienWidth);
                        $(".mask #maskImgShow").attr("height", "auto");
                        var secHeight = $(".mask #maskImgShow").height();
                        if (secHeight > ClienHeightOff) {
                            var ClienHeight = (parseInt(ClienHeightOff) - 30) + "px";
                            $(".mask #maskImgShow").attr("width", "auto");
                            $(".mask #maskImgShow").attr("height", ClienHeight);
                        }
                    } else {
                        if (maskImgHeightyu > ClienHeightOff) {
                            var ClienHeight = (parseInt(ClienHeightOff) - 30) + "px";
                            $(".mask #maskImgShow").attr("width", "auto");
                            $(".mask #maskImgShow").attr("height", ClienHeight);
                            var secWidth = $(".mask #maskImgShow").width();
                            if (secWidth > ClienWidthOff) {
                                var ClienWidth = (parseInt(ClienWidthOff) - 30) + "px";
                                $(".mask #maskImgShow").attr("width", ClienWidth);
                                $(".mask #maskImgShow").attr("height", "auto");
                            }
                        }
                    }


                    var maskImgWdithM = $(".mask #maskImgShow").width();
                    var maskImgWdithH = $(".mask #maskImgShow").height();
                    var ClienHeightCha = ClienHeightOff - maskImgWdithH;
                    $(".maskImg").css("padding-top", ClienHeightCha / 2);
                    var ClienHeightDeta = (parseInt(ClienHeightCha / 2) - 14) + "px";
                    var ClienHeightdetaLeft = (parseInt(maskImgWdithM) - 14) + "px";

                    $(".mask .maskImgDelat").css({ "top": ClienHeightDeta, "left": ClienHeightdetaLeft });
                });
            })
            $("#fellowImg").find("a").css("color", "#469cf1");
        };


        window.onload = function () {
            EditImg();
            $(".maskImgDelat").click(function () {
                $(this).parents(".mask").css("display", "none");
            });

            engine = null;
            if (window.navigator.appName == "Microsoft Internet Explorer") {
                if (document.documentMode) {
                    engine = document.documentMode;
                } else {
                    engine = 5;
                    if (document.compatMode) {
                        if (document.compatMode == "CSS1Compat")
                        { engine = 7; }
                    }
                }
                if (engine <= 7) {
                    alert("您的浏览器版本过低，为了体验更好的效果请升级浏览器或下载最新浏览器使用！");
                }
            }
        }
        $(function () {
            var pagepath = location.pathname + location.search;
            // pagepath = pagepath.toString().substring(pagepath.toString().indexOf("/V"));
            //分页后产生page 
            //if (pagepath.indexOf("page") != "-1") {
            //    pagepath = pagepath.substring(0, pagepath.indexOf("page") - 1);
            //}
            function isContains(substr) {
                ////var ret = substr.trim() == pagepath.trim() ? true : false;
                //var ret = substr == pagepath ? true : false;
                //return ret;
                return pagepath.indexOf(substr) > -1;
            }

            function isRegExpContains(substr) {
                // return new RegExp(substr).test(pagepath);
                return pagepath.indexOf(substr) > -1;
            }

            //function isContains(substr) {
            //    return new RegExp(substr).test(pagepath);
            //}
            if (isContains('HomePage')) {
                addActiveToNav('首页');
            } else if (isContains('LiShiList')) {
                addActiveToNav('校友会');
            } else if (isContains('AssociationList_2')) {
                addActiveToNav('校友会');
            } else if (isContains('AssociationList_4')) {
                addActiveToNav('支持中心');
            } else if (isContains('LocalChapter_2')) {
                addActiveToNav('校友会');
            } else if (isContains('Association_2')) {
                addActiveToNav('校友会');
            } else if (isContains('GZZD_2')) {
                addActiveToNav('校友会');
            } else if (isContains('AssociationListTwo_8')) {
                addActiveToNav('支持中心');
            } else if (isContains('AssociationListTwoJT')) {
                addActiveToNav('支持中心');
            } else if (isContains('Association_8')) {
                addActiveToNav('支持中心');
            } else if (isContains('Recruitment_8')) {
                addActiveToNav('支持中心');
            } else if (isContains('RecruXiang_8')) {
                addActiveToNav('支持中心');
            } else if (isContains('RongYuList_7')) {
                addActiveToNav('校友群芳');
            } else if (isContains('RelationMe_2')) {
                addActiveToNav('校友群芳');
            } else if (isContains('RelationMe_3')) {
                addActiveToNav('校友群芳');
            } else if (isContains('SchoolRcall_4')) {
                addActiveToNav('校友群芳');
            } else if (isContains('SchoolRcall_9')) {
                addActiveToNav('校友捐赠');
            } else if (isContains('NewsList')) {
                addActiveToNav('新闻中心');
            } else if (isContains('Journal')) {
                addActiveToNav('校友刊物');
            } else if (isContains('Contribute')) {
                addActiveToNav('校友刊物');
            } else if (isContains('JuanJZXM')) {
                addActiveToNav('校友捐赠');
            } else if (isContains('JuanJZFS')) {

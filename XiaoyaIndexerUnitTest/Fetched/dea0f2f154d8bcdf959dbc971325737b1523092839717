        <script id="ko-script">
            var url = $.url().attr('path');
            var param = $.url().param();
            param.model = 1;
            $.ajaxSettings.async = false;
            $.getJSON(url,param, function(obj) {
                if (obj.message) alert(obj.message);
                if (obj.success) {
                    try {
                        ko.applyBindings(obj.data);
                        $("[data-bind]").removeAttr("data-bind");
                        $("#ko-script").remove();
                        if(obj.data.pagetitle)
                        {
                            document.title = obj.data.pagetitle;
                        }
                    }
                    catch (e) {
                        alert(e.message);
                    }
                }
            });
        </script>
